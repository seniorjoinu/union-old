{
  "canisters": {
    "voting_token": {
      "build": "cargo build --target wasm32-unknown-unknown --package voting_token --release",
      "candid": "src/voting_token/voting_token.did",
      "wasm": "target/wasm32-unknown-unknown/release/voting_token.wasm",
      "type": "custom"
    },
    "votings": {
      "build": "cargo build --target wasm32-unknown-unknown --package votings --release",
      "candid": "src/votings/votings.did",
      "wasm": "target/wasm32-unknown-unknown/release/votings.wasm",
      "type": "custom"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.0-beta.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
