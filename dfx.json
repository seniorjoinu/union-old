{
  "canisters": {
    "communitea": {
      "build": "./build.sh",
      "candid": "src/communitea/communitea.did",
      "wasm": "target/wasm32-unknown-unknown/release/communitea-opt.wasm",
      "type": "custom"
    },
    "communitea_assets": {
      "dependencies": [
        "communitea"
      ],
      "frontend": {
        "entrypoint": "src/communitea_assets/src/index.html"
      },
      "source": [
        "src/communitea_assets/assets",
        "dist/communitea_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.0-beta.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
