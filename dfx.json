{
  "canisters": {
    "voting_token": {
      "build": "./build-voting_token.sh",
      "candid": "src/voting_token/voting_token.did",
      "wasm": "target/wasm32-unknown-unknown/debug/voting_token-opt.wasm",
      "type": "custom",
      "dependencies": [
        "union_wallet"
      ]
    },
    "voting_manager": {
      "build": "./build-voting_manager.sh",
      "candid": "src/voting_manager/voting_manager.did",
      "wasm": "target/wasm32-unknown-unknown/debug/voting_manager-opt.wasm",
      "type": "custom"
    },
    "union_wallet": {
      "build": "./build-union_wallet.sh",
      "candid": "src/union_wallet/union_wallet.did",
      "wasm": "target/wasm32-unknown-unknown/debug/union_wallet-opt.wasm",
      "type": "custom"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.0",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
