type Error = variant {
    InsufficientBalance;
    AccessDenied;
};

service : {
    "balance_of": (principal, opt int64) -> (nat64) query;
    "mint": (principal, nat64) -> (variant { Ok; Err: Error });
    "send": (principal, nat64) -> (variant { Ok; Err: Error });
    "burn": (nat64) -> (variant { Ok; Err: Error });
}
